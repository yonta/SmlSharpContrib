_require "basis.smi"
_require "ffi.smi"

infixr 0 $

structure Std = struct
  val id : 'a -> 'a
  val $ : (('a -> 'b) * 'a) -> 'b
  val flip : ('a -> 'b -> 'c) -> 'b -> 'a -> 'c
  val curry : ('a * 'b -> 'c) -> 'a -> 'b -> 'c
  val uncurry : ('a -> 'b -> 'c) -> 'a * 'b -> 'c
  val const : 'a -> 'b -> 'a

  (* val dump : 'a -> unit *)

  val version : (int * int * int)
  val version_string : string
end
